<Window x:Name="___No_Name_" x:Class="CreateSteamPicturesJunctionLinks.SetGameName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Set folder name" Height="122" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
	<Grid>
		<TextBox x:Name="TxtInput" Height="23" Margin="10,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="TxtInput_TextChanged"/>
		<Button x:Name="BtnOk" Content="OK" HorizontalAlignment="Left" Margin="129,0,0,11" Width="75" Height="20" VerticalAlignment="Bottom" IsDefault="True" Click="BtnOk_Click" IsEnabled="False"/>
		<Button x:Name="BtnCancel" Content="Cancel" Margin="209,0,10,11" Height="20" VerticalAlignment="Bottom" IsCancel="True" Click="BtnCancel_Click"/>

	</Grid>
</Window>
